{
    "name": "sports-management-app",
    "version": "1.0.0",
    "description": "NFL Sports Management CRUD App with Node, Express, TypeScript, and Prisma",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "nodemon --exec ts-node src/server.ts",
        "build": "tsc",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:seed": "ts-node prisma/seed.ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "nfl",
        "sports",
        "management",
        "api",
        "prisma",
        "typescript",
        "express"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.9",
        "nodemon": "^3.0.1",
        "prisma": "^5.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
