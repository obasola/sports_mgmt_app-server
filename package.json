{
  "name": "sports_mgmt_app-server",
  "version": "1.0.0",
  "description": "Sports Management REST API Server",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json && tsc-alias -p tsconfig.json --verbose",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prod": "npm run build && node dist/index.js",
    "ingest": "ts-node -r tsconfig-paths/register src/cli/ingest.ts",
    "dev:espn": "ts-node -r tsconfig-paths/register src/espn/dev/try-endpoints.ts",
    "dev:ts": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:debug": "nodemon --inspect --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "import:nfl": "tsx src/scripts/import-nfl-season.ts",
    "test:routes": "ts-node -r tsconfig-paths/register src/test/testRoutes.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "cli:importWeek": "ts-node src/cli/importWeek.ts",
    "cli:backfillSeason": "ts-node src/cli/backfillSeason.ts",
    "cli:syncTeams": "ts-node src/cli/syncTeams.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "p-limit": "^7.1.1",
    "yargs": "^18.0.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "prisma": "^6.15.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "directories": {
    "doc": "doc"
  },
  "overrides": {
    "debug": "4.3.7"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
